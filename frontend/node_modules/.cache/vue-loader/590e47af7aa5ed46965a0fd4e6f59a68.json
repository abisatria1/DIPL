gin As\" v-slot=\"{ ariaDescribedby }\">\n        <b-form-radio-group\n          name=\"form.level\"\n          v-model=\"form.levels\"\n          :aria-describedby=\"ariaDescribedby\"\n        >\n          <b-form-radio \n            value=\"participant\"\n          >Participant\n          </b-form-radio>\n          <b-form-radio\n            value=\"campaigner\"\n          >Campaigner\n          </b-form-radio>\n        </b-form-radio-group>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n    </b-form>\n    <b-card class=\"mt-3\" header=\"Form Data Result\">\n      <pre class=\"m-0\">{{ form }}</pre>\n    </b-card>\n  </div>\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        form: {\n          email: '',\n          password: '',\n          levels: '',\n        },\n        show: true\n      }\n    },\n    methods: {\n      onSubmit(event) {\n        event.preventDefault()\n        alert(JSON.stringify(this.form))\n      }\n    }\n  }\n</script>\n<style>\n* {\n  margin: 0;\n  padding: 0;\n}\n\n.container {\n  height: 100vh;\n  /* border: 1px solid black; */\n}\n\n.row {\n  /* border: 1px solid black; */\n  height: 100vh;\n  display: flex;\n  align-items: center;\n}\n\n.login-form {\n  box-shadow: 0px 0px 3px 1px rgba(0, 0, 0, 0.199);\n  padding: 16px;\n  align-items: center;\n  width: 400px;\n  margin: auto;\n}\n</style>"]}]}